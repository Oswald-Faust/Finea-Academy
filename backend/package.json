{
  "name": "finea-backend",
  "version": "1.0.0",
  "description": "Backend API pour l'application Finéa Académie",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "debug": "node --trace-warnings server.js",
    "check-env": "node -e \"console.log('Environment Check:'); console.log('NODE_ENV:', process.env.NODE_ENV); console.log('MONGODB_URI:', process.env.MONGODB_URI ? 'SET' : 'NOT SET'); console.log('JWT_SECRET:', process.env.JWT_SECRET ? 'SET' : 'NOT SET'); console.log('VERCEL:', process.env.VERCEL ? 'TRUE' : 'FALSE');\""
  },
  "keywords": [
    "finea",
    "api",
    "nodejs",
    "express",
    "authentication"
  ],
  "author": "Finéa Académie",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.7.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
